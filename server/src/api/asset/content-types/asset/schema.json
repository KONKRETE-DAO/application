{
  "kind": "collectionType",
  "collectionName": "assets",
  "info": {
    "singularName": "asset",
    "pluralName": "assets",
    "displayName": "Asset",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "uid": {
      "type": "uid",
      "required": true
    },
    "asset_type": {
      "type": "enumeration",
      "enum": [
        "Residential"
      ],
      "required": true
    },
    "apr": {
      "type": "decimal"
    },
    "has_waitlist": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "is_leveraged": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "address": {
      "type": "component",
      "repeatable": false,
      "component": "bloc.address"
    },
    "about": {
      "type": "text"
    },
    "short_description": {
      "type": "string"
    },
    "expected_income": {
      "type": "decimal"
    },
    "rent_start_date": {
      "type": "date"
    },
    "rent_per_token": {
      "type": "decimal"
    },
    "token_price": {
      "type": "decimal"
    },
    "tokens_number": {
      "type": "biginteger"
    },
    "property_type": {
      "type": "enumeration",
      "enum": [
        "Building",
        "Apartment",
        "Retail"
      ]
    },
    "neighborhood": {
      "type": "string"
    },
    "bedrooms_number": {
      "type": "integer"
    },
    "rental_type": {
      "type": "enumeration",
      "enum": [
        "Long-term"
      ]
    },
    "rental_strategy": {
      "type": "string"
    },
    "trust_indice": {
      "type": "integer"
    },
    "highlights": {
      "type": "component",
      "repeatable": true,
      "component": "text.highlights"
    },
    "acquisition_price": {
      "type": "decimal"
    },
    "refurbishment": {
      "type": "decimal"
    },
    "total_price": {
      "type": "decimal"
    },
    "mortgage": {
      "type": "decimal"
    },
    "rental_income": {
      "type": "decimal"
    },
    "net_rental_income": {
      "type": "decimal"
    },
    "internal_rate_of_return": {
      "type": "decimal"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
